<h1>Creando un Proyecto</h1>
<form method="get" action="/pages/create" >
    <div class="form-group">
      <label for="name">Nombre</label>
      <input type="text" class="form-control" name="name"  placeholder="Enter Titulo">
    </div>
    <div class="form-group">
      <label for="description">Descripcion</label>
      <textarea name="description" type="text" class="form-control"></textarea>
    </div>
    <div class="form-group">
      <label for="date_in">Fecha de Ingreso</label>
      <input name="date_in" type="date" class="form-control">
    </div>
    <div class="form-group">
      <label for="date_out">Fecha de Entrega</label>
      <input name="date_out" type="date" class="form-control">
    </div>
    <div class="form-group">
      <label for="state">Estado</label>
      <select id="state" name="state">
        <option value="proposal">Propuesta</option>
        <option value="in_progress">En Progreso</option>
        <option value="finished">Terminado</option>
      </select>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
